    link(rel="stylesheet" href="./css/main.css")

    link(rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons/font/bootstrap-icons.css")
  body
    include ./parts/header.pug
    main(style="margin-top:100px;")
        div.row
            div.col
                div.row.d-flex.justify-content-center
                    div.col-lg-6.col-md-4.col-4.text-center
                        img(src="/img/error-page/404.png")
                div.row.d-flex.justify-content-center
                    div.col-lg-6.col-md-4.col-4.mt-lg-4.mt-md-3.mt-2.text-center
                        h2.fw-bold Oops!
                div.row.d-flex.justify-content-center
                    div.col-lg-6.col-md-4.col-4.mt-lg-4.mt-md-3.mt-2.text-center
                        p.text-muted
                         | We canâ€™t seem to find the page you are looking for
                div.row.d-flex.justify-content-center
                    div.col-lg-10.col-md-8.col-12.mt-lg-4.mt-md-3.mt-2.text-center
                        a.btn.blue-color.text-white.fw-bold.col-lg-2.col-sm-6.rounded-pill(href="index.html")
                            | Back to Homepage
                        a.btn.fw-bold.text-violate.col-lg-2.col-sm-6(href="#")
                            | Read blogs
                div.row.d-flex.justify-content-center
                    div.col-lg-8.col-md-6.col-6.mt-lg-4.mt-md-3.mt-2.text-center
                        h6 Follow us on
                div.row.d-flex.justify-content-center
                    div.col-lg-8.col-md-6.col-6.text-center.p-lg-4.p-md-3.p-2
                        a.btn.rounded-circle.icon-bg.m-lg-4.m-md-3.m-2(href="https://instagram.com" target="_blank" title="Instagram")
                          i.bi.bi-instagram.text-violate
                        a.btn.rounded-circle.icon-bg.m-lg-4.m-md-3.m-2(href="https://facebook.com" target="_blank" title="Facebook")
                          i.bi.bi-facebook.text-violate
                        a.btn.rounded-circle.icon-bg.m-lg-4.m-md-3.m-2(href="https://linkedin.com" target="_blank" title="LinkedIn")
                          i.bi.bi-linkedin.text-violate
                        a.btn.rounded-circle.icon-bg.m-lg-4.m-md-3.m-2(href="https://twitter.com" target="_blank" title="Twitter")
                          i.bi.bi-twitter.text-violate
                        a.btn.rounded-circle.icon-bg.m-lg-4.m-md-3.m-2(href="https://youtube.com" target="_blank" title="YouTube")
                          i.bi.bi-youtube.text-violate
    include ./parts/footer.pug
        script(src='https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js' integrity='sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p' crossorigin='anonymous')
        script(src='https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.min.js' integrity='sha384-Atwg2Pkwv9vp0ygtn1JAojH0nYbwNJLPhwyoVbhoPwBhjQPR5VtM2+xf0Uwh9KtT' crossorigin='anonymous')
        //- script(src="https://unpkg.com/aos@2.3.1/dist/aos.js")
        //- script(src='https://unpkg.com/swiper/swiper-bundle.min.js')
